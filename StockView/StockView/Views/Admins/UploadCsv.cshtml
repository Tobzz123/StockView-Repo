@{
    ViewData["Title"] = "UploadCsv";
}


<form method="post" enctype="multipart/form-data" asp-controller="Admins" asp-action="UploadCsv">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload a Stock CSV using this form:</p>
            <p>Only CSV Stock Files from YahooFinance are allowed!</p>
            <input type="file" name="formFiles" multiple />

        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
    @ViewBag.Message
</form>